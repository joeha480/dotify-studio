<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="rootVBox" minWidth="200.0" spacing="10.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.ui.prefs.ImportSettingsController">
	<children>
      <HBox>
         <children>
            <Label minWidth="100.0" text="%label-target-locale">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Label>
            <Region HBox.hgrow="ALWAYS" />
            <ComboBox fx:id="selectLocale" maxWidth="-Infinity" prefWidth="210.0" />
         </children>
      </HBox>
      <HBox fx:id="hboxOutputFormat">
         <children>
	         <Label minWidth="100.0" text="!Output format">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font></Label>
            <Region HBox.hgrow="ALWAYS" />
	         <ComboBox fx:id="selectOutputFormat" maxWidth="-Infinity" prefWidth="210.0" />
         </children>
      </HBox>
	</children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
